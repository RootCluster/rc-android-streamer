<resources>
    <declare-styleable name="streamer">
        <!--指示器位置-->
        <attr name="streamer_indicator_direction">
            <enum name="left">0</enum>
            <enum name="center">1</enum>
            <enum name="right">2</enum>
        </attr>
        <!--指示器风格-->
        <attr name="streamer_indicator_type">
            <enum name="number">0</enum>
            <enum name="circular">1</enum>
            <enum name="other">2</enum>
        </attr>
        <!--指示器颜色-->
        <attr name="streamer_indicator_color" format="integer" />
        <!--动画时间-->
        <attr name="streamer_time_animator" format="integer" />
    </declare-styleable>
</resources>